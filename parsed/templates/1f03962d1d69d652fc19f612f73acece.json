{"components/cart/minicart-footer":"<footer class=\"minicart-footer\">\n  {{#if cart.discount.value \"<\" 0}}\n    <div class=\"minicart-footer-row\">\n      <div class=\"minicart-footer-label\">\n        {{lang 'cart.minicart.discount'}}\n      </div>\n      <div class=\"minicart-footer-price\">\n        {{cart.discount.formatted}}\n      </div>\n    </div>\n  {{/if}}\n\n  <div class=\"minicart-footer-row\">\n    <div class=\"minicart-footer-label\">\n      {{lang 'cart.minicart.total'}}\n    </div>\n    <div class=\"minicart-footer-price\">\n      {{cart.sub_total.formatted}}\n    </div>\n  </div>\n\n  <a\n    href=\"{{urls.cart}}\"\n    class=\"button button-primary button-fullwidth button-minicart-viewcart\"\n  >\n    <span>{{lang 'cart.minicart.view_cart'}}</span>\n  </a>\n  <a\n    href=\"{{urls.checkout.single_address}}\"\n    class=\"button button-secondary button-fullwidth\"\n  >\n    {{lang 'cart.minicart.checkout'}}\n  </a>\n  {{#if cart.show_multiple_address_shipping}}\n    <div class=\"multiple-checkout\">\n      <a class=\"multiple-checkout-link\" href=\"{{urls.checkout.multiple_address}}\">\n        {{lang 'cart.checkout_multiple'}}\n      </a>\n    </div>\n  {{/if}}\n</footer>\n"}
