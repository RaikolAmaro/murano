{"components/footer/payment-icons":"<div class=\"footer-payment-icons\">\n  {{#if theme_settings.geotrust-ssl-common-name}}\n    {{> core/geotrust-ssl-seal}}\n  {{/if}}\n  <ul class=\"footer-payment-icons-list\">\n    {{#if theme_settings.show_visa_icon}}\n      <li>\n        <svg class=\"footer-payment-icon visa\" role=\"presentation\">\n          <use xlink:href=\"#icon-visa\"></use>\n        </svg>\n      </li>\n    {{/if}}\n\n    {{#if theme_settings.show_mastercard_icon}}\n      <li>\n        <svg class=\"footer-payment-icon mastercard\" role=\"presentation\">\n          <use xlink:href=\"#icon-mastercard\">\n        </svg>\n      </li>\n    {{/if}}\n\n    {{#if theme_settings.show_amex_icon}}\n      <li>\n        <svg class=\"footer-payment-icon american-express\" role=\"presentation\">\n          <use xlink:href=\"#icon-american-express\">\n        </svg>\n      </li>\n    {{/if}}\n\n    {{#if theme_settings.show_discover_icon}}\n      <li>\n        <svg class=\"footer-payment-icon discover\" role=\"presentation\">\n          <use xlink:href=\"#icon-discover\">\n        </svg>\n      </li>\n    {{/if}}\n\n    {{#if theme_settings.show_paypal_icon}}\n      <li>\n        <svg class=\"footer-payment-icon paypal\" role=\"presentation\">\n          <use xlink:href=\"#icon-paypal\">\n        </svg>\n      </li>\n    {{/if}}\n\n    {{#if theme_settings.show_apple_pay_icon}}\n      <li>\n        <svg class=\"footer-payment-icon apple-pay\" role=\"presentation\">\n          <use xlink:href=\"#icon-apple-pay\">\n        </svg>\n      </li>\n    {{/if}}\n  </ul>\n</div>\n","core/geotrust-ssl-seal":"<table\n  class=\"geotrust-ssl-seal-table\"\n  width=\"135\"\n  border=\"0\"\n  cellpadding=\"2\"\n  cellspacing=\"0\"\n  title=\"{{lang 'core.geotrust.click_to_verify'}}\"\n>\n  <tr>\n    <td width=\"135\" align=\"center\" valign=\"top\">\n      <script type=\"text/javascript\" src=\"https://seal.geotrust.com/getgeotrustsslseal?host_name={{theme_settings.geotrust-ssl-common-name}}&amp;size={{theme_settings.geotrust-ssl-seal-size}}&amp;lang={{locale_name}}\"></script><br />\n      <a\n        href=\"http://www.geotrust.com/ssl/\"\n        target=\"_blank\"\n        style=\"color:#000000; text-decoration:none; font:bold 7px verdana,sans-serif; letter-spacing:.5px; text-align:center; margin:0px; padding:0px;\"\n      ></a>\n    </td>\n  </tr>\n</table>\n"}
