{"components/product/tabs/reviews":"<section\n  class=\"product-tabs-reviews product-tabs-section\"\n  id=\"write_review\"\n  data-tab-content\n>\n  <h1 class=\"product-review-title\">{{lang 'product.tabs.reviews' num=product.reviews.total}}</h1>\n  {{#if product.reviews.list.length}}\n    <button\n      class=\"review-link link-button\"\n      data-review-link\n    >\n      {{lang 'product.tabs.leave_review'}}\n    </button>\n    {{#each product.reviews.list}}\n      <article class=\"product-tab-review\">\n        <div class=\"product-rating\">\n          <div class=\"rating-stars-readonly\" aria-hidden=\"true\">\n            {{#enumerate 1 5}}\n              {{#compare ../rating this operator='>='}}\n                <svg class=\"rating-star rating-star-full\" role=\"presentation\">\n                  <use xlink:href=\"#icon-star\"></use>\n                </svg>\n              {{else}}\n                <svg class=\"rating-star rating-star-empty\" role=\"presentation\">\n                  <use xlink:href=\"#icon-star\"></use>\n                </svg>\n              {{/compare}}\n            {{/enumerate}}\n          </div>\n        </div>\n        <div class=\"product-tab-review-name\">{{name}}</div>\n        <h1 class=\"product-tab-review-title\">{{title}}</h1>\n        <div class=\"product-tab-review-description\">{{text}}</div>\n      </article>\n    {{/each}}\n  {{else}}\n    <p class=\"product-tabs-no-reviews\">\n      {{lang 'product.reviews.no_reviews'}}\n      <button\n        class=\"no-review-link link-button\"\n        data-review-link\n      >\n        {{lang 'product.reviews.leave_review'}}\n      </button>\n    </p>\n  {{/if}}\n  {{> components/reviews/review-modal}}\n</section>\n","components/reviews/review-modal":"<section id=\"modal-review-form\" class=\"review-modal-container\">\n  <button class=\"modal-close\" data-review-modal-close>\n    <svg class=\"review-close-icon\" title=\"Close review modal\" role=\"img\">\n      <use xlink:href=\"#icon-close\"></use>\n    </svg>\n  </button>\n\n  <div class=\"modal-body\">\n    <div class=\"review-form-column\">\n      <h1 class=\"review-form-heading\">\n        {{lang 'product.reviews.write_review'}}\n      </h1>\n      <form\n        class=\"form\"\n        id=\"form-leave-a-review\"\n        action=\"{{urls.product.post_review}}\"\n        method=\"post\"\n      >\n        <input type=\"hidden\" name=\"product_id\" value=\"{{product.id}}\">\n        <input type=\"hidden\" name=\"action\" value=\"post_review\">\n        {{#if product.reviews.throttleToken}}\n          <input\n            type=\"hidden\"\n            name=\"throttleToken\"\n            value=\"{{product.reviews.throttleToken}}\"\n          />\n        {{/if}}\n\n        {{!-- Clickable Stars --}}\n        <div class=\"form-field\">\n          <div class=\"form-label-text\">\n            {{lang 'product.reviews.rating_label'}}\n          </div>\n          <fieldset class=\"input-stars form-fieldset\">\n            <input\n              type=\"radio\"\n              id=\"stars-5\"\n              class=\"show-for-sr\"\n              name=\"revrating\"\n              value=\"5\"\n              checked\n            >\n            <label\n              for=\"stars-5\"\n              class=\"star\"\n              title=\"{{lang 'product.reviews.stars_count' NUM=5}}\"\n            >\n              <svg\n                class=\"star star-full\"\n                title=\"{{lang 'product.reviews.stars_count' NUM=5}}\"\n                role=\"img\"\n              >\n                <use xlink:href=\"#icon-star\">\n              </svg>\n              <span class=\"span show-for-sr\">\n                {{lang 'product.reviews.stars_count' NUM=5}}\n              </span>\n            </label>\n            <input\n              type=\"radio\"\n              id=\"stars-4\"\n              class=\"show-for-sr\"\n              name=\"revrating\"\n              value=\"4\"\n            >\n            <label\n              for=\"stars-4\"\n              class=\"star\"\n              title=\"{{lang 'product.reviews.stars_count' NUM=4}}\"\n            >\n              <svg\n                class=\"star star-full\"\n                title=\"{{lang 'product.reviews.stars_count' NUM=4}}\"\n                role=\"img\"\n              >\n                <use xlink:href=\"#icon-star\">\n              </svg>\n              <span class=\"span show-for-sr\">\n                {{lang 'product.reviews.stars_count' NUM=4}}\n              </span>\n            </label>\n            <input\n              type=\"radio\"\n              id=\"stars-3\"\n              class=\"show-for-sr\"\n              name=\"revrating\"\n              value=\"3\"\n            >\n            <label\n              for=\"stars-3\"\n              class=\"star\"\n              title=\"{{lang 'product.reviews.stars_count' NUM=3}}\"\n            >\n              <svg\n                class=\"star star-full\"\n                title=\"{{lang 'product.reviews.stars_count' NUM=3}}\"\n                role=\"img\"\n              >\n                <use xlink:href=\"#icon-star\">\n              </svg>\n              <span class=\"span show-for-sr\">\n                {{lang 'product.reviews.stars_count' NUM=3}}\n              </span>\n            </label>\n            <input\n              type=\"radio\"\n              id=\"stars-2\"\n              class=\"show-for-sr\"\n              name=\"revrating\"\n              value=\"2\"\n            >\n            <label\n              for=\"stars-2\"\n              class=\"star\"\n              title=\"{{lang 'product.reviews.stars_count' NUM=2}}\"\n            >\n              <svg\n                class=\"star star-full\"\n                title=\"{{lang 'product.reviews.stars_count' NUM=2}}\"\n                role=\"img\"\n              >\n                <use xlink:href=\"#icon-star\">\n              </svg>\n              <span class=\"span show-for-sr\">\n                {{lang 'product.reviews.stars_count' NUM=2}}\n              </span>\n            </label>\n            <input\n              type=\"radio\"\n              id=\"stars-1\"\n              class=\"show-for-sr\"\n              name=\"revrating\"\n              value=\"1\"\n            >\n            <label\n              for=\"stars-1\"\n              class=\"star\"\n              title=\"{{lang 'product.reviews.stars_count' NUM=1}}\"\n            >\n              <svg\n                class=\"star star-full\"\n                title=\"{{lang 'product.reviews.stars_count' NUM=1}}\"\n                role=\"img\"\n              >\n                <use xlink:href=\"#icon-star\">\n              </svg>\n              <span class=\"span show-for-sr\">\n                {{lang 'product.reviews.stars_count' NUM=1}}\n              </span>\n            </label>\n          </fieldset>\n        </div>\n\n        {{!-- Name --}}\n        {{> core/forms/common/text\n          label=(lang 'product.reviews.form.name')\n          name=\"revfromname\"\n          id=\"revfromname\"\n          required=\"true\"\n          prefill=(lang 'product.reviews.form.name')\n        }}\n\n        {{!-- Email --}}\n        {{> core/forms/common/email\n          label=(lang 'product.reviews.form.email')\n          name=\"email\"\n          id=\"rating-email\"\n          required=\"true\"\n          prefill=(lang 'product.reviews.form.email')\n        }}\n\n        {{!-- Subject --}}\n        {{> core/forms/common/text\n          label=(lang 'product.reviews.form.subject')\n          name=\"revtitle\"\n          id=\"rating-title\"\n          required=\"true\"\n          prefill=(lang 'product.reviews.form.subject')\n        }}\n\n        {{!-- Comments --}}\n        {{> core/forms/common/multiline\n          label=(lang 'product.reviews.form.comments')\n          name=\"revtext\"\n          id=\"rating-comment\"\n          required=\"true\"\n          rows=6\n          prefill=(lang 'product.reviews.form.comments')\n        }}\n\n        {{!-- Recaptcha V2 --}}\n        {{{product.reviews.recaptcha.markup}}}\n\n        <input\n          type=\"submit\"\n          class=\"form-submit button button-primary\"\n          value=\"{{lang 'product.reviews.form.submit'}}\"\n        >\n      </form>\n    </div>\n\n  </div>\n</section>\n","core/forms/common/text":"\n<div class=\"form-field form-field-common form-field-text {{#if required}} form-required{{/if}}\" id=\"{{id}}\">\n  <label class=\"form-label\">\n    <span class=\"form-field-title\">\n      {{label}}\n      {{#if required}}<span class=\"required-text\">{{lang 'common.required'}}</span>{{/if}}\n    </span>\n    <span class=\"form-field-control\">\n      <input class=\"form-input\"\n        id=\"input-{{id}}\"\n        name=\"{{name}}\"\n        type=\"text\"\n        placeholder=\"{{prefill}}\"\n        {{#if value}}value=\"{{value}}\"{{/if}}\n        {{#if private_id}}data-field-type=\"{{private_id}}\"{{/if}}\n        {{#if required}}required{{/if}}\n        aria-required=\"{{required}}\">\n    </span>\n  </label>\n</div>\n","core/forms/common/email":"\n<div class=\"form-field form-field-common form-field-email {{#if required}} form-required{{/if}}\" id=\"{{id}}\">\n  <label class=\"form-label\">\n    <span class=\"form-field-title\">\n      {{label}}\n      {{#if required}}<span class=\"required-text\">{{lang 'common.required'}}</span>{{/if}}\n    </span>\n    <span class=\"form-field-control\">\n      <input class=\"form-input\"\n        id=\"input-{{id}}\"\n        name=\"{{name}}\"\n        type=\"email\"\n        placeholder=\"{{prefill}}\"\n        {{#if value}}value=\"{{value}}\"{{/if}}\n        {{#if private_id}}data-field-type=\"{{private_id}}\"{{/if}}\n        {{#if required}}required{{/if}}\n        aria-required=\"{{required}}\">\n    </span>\n  </label>\n</div>\n","core/forms/common/multiline":"{{!\n  Textarea form component\n\n  @param validation_message string\n         Custom validation message\n}}\n<div class=\"form-field form-field-common form-field-multiline {{#if required}}form-required{{/if}}\" id=\"{{id}}\">\n  <label class=\"form-label\">\n    <span class=\"form-field-title\">\n      {{label}}\n      {{#if required}}<span class=\"required-text\">{{lang 'common.required'}}</span>{{/if}}\n    </span>\n    <span class=\"form-field-control\">\n      <textarea\n        class=\"form-input\"\n        id=\"textarea-{{id}}\"\n        name=\"{{name}}\"\n        placeholder=\"{{prefill}}\"\n        rows=\"{{rows}}\"\n        {{#if required}}\n          required\n          {{#if validation_message}}\n            data-vd-message-required=\"{{validation_message}}\"\n          {{/if}}\n        {{/if}}\n        aria-required=\"{{required}}\">{{value}}</textarea>\n    </span>\n  </label>\n</div>\n"}
